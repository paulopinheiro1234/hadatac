cogat:CAO_00922 a owl:Class ;
    rdfs:label "Same-different task" ;
    dc:contributor "Melissa Lewis" ;
    dc:date "2012-05-16 21:34:20"^^<xsd:dateTime> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty sio:hasPart ;
