    rdfs:label "Stop signal walking task with stroop" ;
    dc:contributor "Vanessa Sochat" ;
    dc:date "2015-07-30 0:52:51"^^<xsd:dateTime> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty sio:hasPart ;
            owl:someValuesFrom cogat:CAO_02190 ],
